        int distance = 3;
        RaycastHit hit;
        Ray ray = cam.ScreenPointToRay(Input.mousePosition);

        if (Physics.Raycast(ray, out hit, distance))
        {
            if (hit.transform.tag == "")
            {
                if (Input.GetKeyDown(KeyCode.E))
                {
                    //Code
                }
            }

        }
